  "project": "backend-serverless",
  "scripts": {
    "prebuild": "rimraf dist",
    "prepare": "husky install",
    "preinstall": "npx npm-force-resolutions",
    "compile": "npm run-script build && npx tsc",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "npm run task:prepare-env:local && nest start",
    "start:local": "cross-env NODE_ENV=local nodemon",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test:watch": "jest --watch --config ./jest.config.json",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "cross-env NODE_ENV=local jest --config ./e2e-tests/jest-e2e.json",
    "sls:offline": "cross-env NODE_ENV=development SLS_DEBUG=* sls offline --config ./sls/serverless.dev.yml start",
    "sls:pre": "rimraf .serverless && rimraf dist && npm run build",
    "sls:prune": "sls prune -n 10 --verbose",
    "sls:deploy:dev": "npm run sls:pre && cross-env NODE_ENV=dev sls deploy --config ./sls/serverless.dev.yml --verbose",
    "sls:deploy:stg": "npm run sls:pre && cross-env NODE_ENV=stg sls deploy --config ./sls/serverless.stg.yml --verbose",
    "sls:deploy:prd": "npm run sls:pre && cross-env NODE_ENV=prd sls deploy --config ./sls/serverless.prod.yml --verbose",
    "sls:remove": "sls remove --config ./sls/serverless.dev.yml",
    "tscheck:w": "npm run tscheck -- --watch --config ./jest.config.json",
    "test:r": "jest --runInBand --config ./jest.config.json",
    "test:w": "npm run test:r -- --watch --config ./jest.config.json",
    "test:cov": "jest --detectOpenHandles  --coverage --verbose --config ./jest.config.json",
    "test:d": "node --inspect-brk node_modules/jest/bin/jest --runInBand --config ./jest.config.json",
    "test:e2e:w": "npm run test:e2e -- --watch",
    "test:api:w": "run-p start:dev test:e2e:w",
    "webpack": "webpack --config webpack.config.js",
    "upload-s3": "aws s3 cp ./doc s3://graphql-document --recursive",
    "start:hmr": "node dist/main.js",
    "lint:fix": "tslint --fix -p tsconfig.json -c tslint.json",
    "codegen": "ts-node src/generator/generate-typings.ts",
    "gql-gen": "gqlg --schemaFilePath ./src/schema.gql --destDirPath ./e2e-tests/graphql --depthLimit 10"
  },
  "graphdoc": {
    "endpoint": "http://localhost:4000/graphql",
    "output": "./doc"
  },
